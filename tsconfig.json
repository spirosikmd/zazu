{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "types": [
    "node",
    "jasmine",
    "core-js",
    "randomstring",
    "angular-hotkeys"
  ],
  "exclude": [
    "node_modules"
  ]
}
